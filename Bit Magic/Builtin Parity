//_builtin_parity(x): This function is used to check the parity of a number. This function returns true(1) if the number has odd parity else it returns false(0) for even parity.
#include <bits/stdc++.h>

using namespace std;

int main()
{
  int n;
  cin>>n;
  if(__builtin_parity(n))
  cout<<"Yes";
  else cout<<"No";

    return 0;
}
